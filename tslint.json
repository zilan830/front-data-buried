{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "ordered-imports": false,
        camelcase: false,
    },
    "linterOptions": {
        "exclude": [
            "**/*.json",
            "webpack.**.js",
            "node_modules"
        ]
    }
}